/* pages/search-search/search-search.wxss */
.head {
    margin-top: 20rpx;
    width: 100%;
    height: 120;
    background-color: #f3f3f3;
}

.headInput {
    background-color: #fff;
    width: 690rpx;
    height: 80rpx;
    border-radius: 15rpx;
    display: flex;
    align-items: center;
    padding-left: 25rpx;
    box-shadow: 0 8rpx 10rpx #d6d6d6;
}

.headInput image {
    width: 30rpx;
    height: 30rpx;
}

.headInput input {
    width: 550rpx;
    padding: 0rpx 20rpx;
    font-size: 30rpx;
    margin-right: 20rpx;
}
